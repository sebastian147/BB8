{

}
void		MoverMotores		( void )
{

}
void		ActualizarMotores	(void)
{
	switch( accion )
	{
		case Frenar:
			    break;
		case MoverAdelante:
				break;
		case MoverAtras:
				break;
		case MoverIzquierda:
				break;
		case MoverDerecha:
				break;
		case GirarDerecha:
				break;
		case GirarIzquierda:
				break;
	}
}
